')\n       //Save the book details\n       axios.post('/api/books',\n            {\n            bookId: book.id,    \n            imgLoc: book.volumeInfo.imageLinks.thumbnail,\n            title: book.volumeInfo.title, \n            authors: book.volumeInfo.authors,\n            description: book.volumeInfo.description,\n            link: book.selfLink,\n            userID: userData.user.id\n            },\n            {\n                headers: { \"x-auth-token\": localStorage.getItem(\"auth-token\") },\n              }\n        )\n        .then(res => {\n            console.log(res);\n        })\n   \n    }\n\n\n    return (\n        <div className=\"responsive-table\">\n          \n              <table>\n                <thead>\n                <tr>     \n                    <th></th>\n                    <th>Title</th>\n                    <th>Author(s)</th>\n                    <th>Description</th>\n                </tr>\n                </thead>\n                <tbody>\n                    {filteredBooks && filteredBooks.map((book) => (    \n                        <tr className='table-row' key={book.id}>\n                            <td className=\"col s4\">\n                            {book.volumeInfo.imageLinks && <img src={book.volumeInfo.imageLinks.thumbnail} alt={\"cover\" + book.volumeInfo.title}/>}\n                            </td>\n                            <td className=\"s2\">{book.volumeInfo.title} </td>\n                            <td className=\"s2\">{book.volumeInfo.authors?book.volumeInfo.authors.map((value,index) => \n                                    (<p key={index}>{value}</p>)):null}\n                            </td>\n                            <td className=\"s4\">{book.volumeInfo.description}</td>\n                            <td className=\"s4\">\n                                {props.calledby === 'search' ? <button type=\"button\" onClick={e => {handleSaveClick(e,book)}}>Save</button>:null}\n                            </td>\n                        </tr>\n                    ))\n                    }\n                </tbody>\n            </table>\n          </div>\n \n\n    )\n}\n\nexport default SearchDisplay\n","/Users/kwelch/MERN_BOOT_CAMP/homework/21-Homework/BookMarker/client/src/components/pages/SavedDisplay.js",[],"/Users/kwelch/MERN_BOOT_CAMP/homework/21-Homework/BookMarker/client/src/components/Context/UserContext.js",[],"/Users/kwelch/MERN_BOOT_CAMP/homework/21-Homework/BookMarker/client/src/components/pages/Login.js",[],"/Users/kwelch/MERN_BOOT_CAMP/homework/21-Homework/BookMarker/client/src/components/pages/Register.js",["92","93","94","95","96","97"],"/Users/kwelch/MERN_BOOT_CAMP/homework/21-Homework/BookMarker/client/src/components/pages/Confirmation.js",[],"/Users/kwelch/MERN_BOOT_CAMP/homework/21-Homework/BookMarker/client/src/components/pages/ConfirmAccount.js",[],{"ruleId":"98","replacedBy":"99"},{"ruleId":"100","replacedBy":"101"},{"ruleId":"102","severity":1,"message":"103","line":33,"column":8,"nodeType":"104","endLine":33,"endColumn":18,"suggestions":"105"},{"ruleId":"106","severity":1,"message":"107","line":3,"column":8,"nodeType":"108","messageId":"109","endLine":3,"endColumn":16},{"ruleId":"106","severity":1,"message":"110","line":11,"column":23,"nodeType":"108","messageId":"109","endLine":11,"endColumn":34},{"ruleId":"106","severity":1,"message":"111","line":15,"column":12,"nodeType":"108","messageId":"109","endLine":15,"endColumn":19},{"ruleId":"106","severity":1,"message":"112","line":16,"column":12,"nodeType":"108","messageId":"109","endLine":16,"endColumn":21},{"ruleId":"106","severity":1,"message":"110","line":16,"column":23,"nodeType":"108","messageId":"109","endLine":16,"endColumn":34},{"ruleId":"106","severity":1,"message":"113","line":10,"column":11,"nodeType":"108","messageId":"109","endLine":10,"endColumn":17},{"ruleId":"98","replacedBy":"114"},{"ruleId":"100","replacedBy":"115"},{"ruleId":"98","replacedBy":"116"},{"ruleId":"100","replacedBy":"117