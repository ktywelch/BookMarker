playName: ''});\r\n\r\n  const history = useHistory();\r\n\r\n  const onChange = (e) => {\r\n    setForm({ ...form, [e.target.name]: e.target.value });\r\n  };\r\n\r\n  const submit = async (e) => {\r\n    e.preventDefault();\r\n    //  if(validEmail(form.email) && validPassword(form.password) &&   passCheck(form.password,form.passwordCheck)){\r\n          try {\r\n            console.log(form);\r\n            const newUser = await axios.post(\"/users/register\", form);\r\n            history.push('/confirmation')\r\n          } catch (err) {\r\n            toast.error(err.response);\r\n          }\r\n      //} \r\n  };\r\n\r\n  function validateEmail(email) {\r\n    const re = /^(([^<>()[\\]\\\\.,;:\\s@\\\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\\\"]+)*)|(\\\".+\\\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\r\n    return re.test(email);\r\n  }\r\n\r\n  const validEmail = (str) => {\r\n    if (validateEmail(str)) {\r\n      return true;\r\n    }\r\n    toast.error('Password is less than 8 characters');\r\n    setTimeout(() =>  setForm({...form, email: ''}), 3000);\r\n     return false;\r\n  };\r\n\r\n\r\n  const validPassword = (str) => {\r\n    if (str.length >= 8) {\r\n      return true;\r\n    }\r\n    toast.error('Password is less than 8 characters');\r\n    setTimeout(() =>  setForm({...form, password: '', passwordCheck: ''}), 3000);\r\n    return false;\r\n  };\r\n\r\n\r\n  \r\n  const passCheck = (str1,str2) => {\r\n    if (str1 === str2) {\r\n      return true;\r\n    }\r\n    toast.error('Passwords do not match please try again');\r\n    setTimeout(() =>  setForm({...form, password: '', passwordCheck: ''}), 3000);\r\n    return false;\r\n  };\r\n\r\n\r\n\r\n  return (\r\n    <div className=\"container\" style={{maxWidth: 600}}>\r\n      {msg}\r\n      <form onSubmit={submit}>\r\n      <label>Email</label>\r\n        <input onChange={onChange} type=\"text\" name=\"email\" value={form.email} />\r\n        <label>Password</label>\r\n        <input onChange={onChange} type=\"password\" name=\"password\"  value={form.password}/>\r\n        <label>Password Check</label>\r\n        <input onChange={onChange} type=\"password\" name=\"passwordCheck\"  value={form.passwordCheck}/>\r\n        <label>Display Name</label>\r\n        <input onChange={onChange} type=\"text\" name=\"displayName\"  value={form.displayName}/>\r\n        <button type=\"submit\">Register</button>\r\n      </form>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Register;\r\n","/Users/kwelch/MERN_BOOT_CAMP/homework/21-Homework/BookMarker/client/src/components/pages/Confirmation.js",[],"/Users/kwelch/MERN_BOOT_CAMP/homework/21-Homework/BookMarker/client/src/components/pages/ConfirmAccount.js",[],{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","replacedBy":"105"},{"ruleId":"106","severity":1,"message":"107","line":33,"column":8,"nodeType":"108","endLine":33,"endColumn":18,"suggestions":"109"},{"ruleId":"110","severity":1,"message":"111","line":3,"column":8,"nodeType":"112","messageId":"113","endLine":3,"endColumn":16},{"ruleId":"110","severity":1,"message":"114","line":12,"column":23,"nodeType":"112","messageId":"113","endLine":12,"endColumn":34},{"ruleId":"110","severity":1,"message":"115","line":25,"column":15,"nodeType":"112","messageId":"113","endLine":25,"endColumn":21},{"ruleId":"106","severity":1,"message":"116","line":49,"column":10,"nodeType":"108","endLine":49,"endColumn":34,"suggestions":"117"},{"ruleId":"110","severity":1,"message":"114","line":16,"column":23,"nodeType":"112","messageId":"113","endLine":16,"endColumn":34},{"ruleId":"110","severity":1,"message":"118","line":10,"column":11,"nodeType":"112","messageId":"113","endLine":10,"endColumn":17},{"ruleId":"102","replacedBy":"119"},{"ruleId":"104","replacedBy":"120"},{"ruleId":"102","replacedBy":"121"},{"ruleId":"104","replacedBy":"122"},{"ruleId":"110","severity":1,"message":"114","line":7,"column":23,"nodeType":"112","messageId":"113","e