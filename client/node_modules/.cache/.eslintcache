\n              }\n        )\n        .then(res => {\n            console.log(res);\n        })\n   \n    }\n\n\n    return (\n        <div className=\"responsive-table\">\n          \n              <table>\n                <thead>\n                <tr>     \n                    <th></th>\n                    <th>Title</th>\n                    <th>Author(s)</th>\n                    <th>Description</th>\n                </tr>\n                </thead>\n                <tbody>\n                    {filteredBooks && filteredBooks.map((book) => (    \n                        <tr className='table-row' key={book.id}>\n                            <td className=\"col s4\">\n                            {book.volumeInfo.imageLinks && <img src={book.volumeInfo.imageLinks.thumbnail} alt={\"cover\" + book.volumeInfo.title}/>}\n                            </td>\n                            <td className=\"s2\">{book.volumeInfo.title} </td>\n                            <td className=\"s2\">{book.volumeInfo.authors?book.volumeInfo.authors.map((value,index) => \n                                    (<p key={index}>{value}</p>)):null}\n                            </td>\n                            <td className=\"s4\">{book.volumeInfo.description}</td>\n                            <td className=\"s4\">\n                                {props.calledby === 'search' ? <button type=\"button\" onClick={e => {handleSaveClick(e,book)}}>Save</button>:null}\n                            </td>\n                        </tr>\n                    ))\n                    }\n                </tbody>\n            </table>\n          </div>\n \n\n    )\n}\n\nexport default SearchDisplay\n","/Users/kwelch/MERN_BOOT_CAMP/homework/21-Homework/BookMarker/client/src/components/pages/SavedDisplay.js",[],"/Users/kwelch/MERN_BOOT_CAMP/homework/21-Homework/BookMarker/client/src/components/Context/UserContext.js",[],"/Users/kwelch/MERN_BOOT_CAMP/homework/21-Homework/BookMarker/client/src/components/pages/Logi